                        }
                    })
                    .setView([37.4853, 126.9532], 10);
                var markers = [];
                function myMap() {
                    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
                        maxZoom: 19,
                        attribution: 'Â© OpenStreetMap contributors'
                    }).addTo(map);
                    L.Control.geocoder().addTo(map);
                }
                myMap();

            </script>

